{
  "ApiPaths": {
    "PokemonCryLatestTemplate": "https://raw.githubusercontent.com/PokeAPI/cries/main/cries/pokemon/latest/{0}.ogg",
    "PokemonCryLegacyTemplate": "https://raw.githubusercontent.com/PokeAPI/cries/main/cries/pokemon/legacy/{0}.ogg",

    "PokemonOfficialArtworkTemplate": "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/official-artwork/{0}.png",
    "PokemonDreamWorldArtworkTemplate": "https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/other/dream-world/{0}.svg",

    "PokemonSpecies": "pokemon-species?limit={limit}&offset={offset}",
    "StandardPokeballs": "item-category/standard-balls",
    "SpecialPokeballs": "item-category/special-balls"
